{
  "env": {
    "browser": true,
    "es2021": true,
    "jasmine": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-console": "warn",
    "prefer-const": "error",
    "eqeqeq": ["error", "always"],
    "curly": ["error", "all"]
  },
  "globals": {
    "Animal": "readonly",
    "Dog": "readonly"
  }
}
